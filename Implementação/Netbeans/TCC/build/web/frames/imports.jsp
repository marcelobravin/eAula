<script language="JavaScript1.1">
function interruptor(texto,valor) {
    if (valor==1){
        document.getElementById("texto"+texto).style.display="inherit";
        document.getElementById("botao"+texto).innerHTML ='<a class="sinal_menos" onClick="interruptor('+texto+',0)"></a>';
    }
    if (valor==0){
        document.getElementById("texto"+texto).style.display="none";
        document.getElementById("botao"+texto).innerHTML ='<a class="sinal_mais" onClick="interruptor('+texto+',1)"></a>';
    }
}
</script>


















